                    <input asp-for="SignatureDate" class="form-control" />
                    <span asp-validation-for="SignatureDate" class="text-danger"></span>
                </div>
            </div>
            <div class="row align-items-md-center border">
                <div class="form-group col-md-2">
                    <label>Configuration:</label>
                </div>
                <div class="form-group col-md-2">
                    <div class="checkbox">
                        <label>
                            <input asp-for="ConfigurationHardware" /> @Html.DisplayNameFor(model => model.ConfigurationHardware)
                        </label>
                    </div>
                </div>
                <div class="form-group col-md-2">
                    <div class="checkbox">
                        <label>
                            <input asp-for="ConfigurationSoftware" /> @Html.DisplayNameFor(model => model.ConfigurationSoftware)
                        </label>
                    </div>
