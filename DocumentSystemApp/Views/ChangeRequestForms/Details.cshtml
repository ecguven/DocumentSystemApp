@model DocumentSystemApp.Data.Entities.ChangeRequestForm

@{
    ViewData["Title"] = "Details";
}

<h2>Details</h2>

<div>
    <h4>ChangeRequestForm</h4>
    <hr />
    <div class="container">
        <form contenteditable="false">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <div class="row align-items-start border">
                <div class="form-group col-md-12">
                    <label asp-for="CompanyId" class="control-label"></label>
                    <input asp-for="CompanyId" class="form-control" />
                    <span asp-validation-for="CompanyId" class="text-danger"></span>
                </div>
            </div>
            <div class="row align-items-md-center border">
                <div class="col-md-4">
                    <div class="form-group">
                        <label asp-for="SPRS" class="control-label"></label>
                        <input asp-for="SPRS" class="form-control" />
                        <span asp-validation-for="SPRS" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label asp-for="Version" class="control-label"></label>
                        <input asp-for="Version" class="form-control" />
                        <span asp-validation-for="Version" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label asp-for="ChangeRequestID" class="control-label"></label>
                        <input asp-for="ChangeRequestID" class="form-control" />
                        <span asp-validation-for="ChangeRequestID" class="text-danger"></span>
                    </div>
                </div>
            </div>
            <div class="row align-items-md-center border">
                <div class="form-group col-md-12">
                    <label asp-for="ChangeRequestDescrpition" class="control-label"></label>
                    <textarea asp-for="ChangeRequestDescrpition" class="form-control"></textarea>
                    <span asp-validation-for="ChangeRequestDescrpition" class="text-danger"></span>
                </div>
            </div>
            <div class="row align-items-md-center border">
                <div class="form-group col-md-12">
                    <label asp-for="ChangeReason" class="control-label"></label>
                    <textarea asp-for="ChangeReason" class="form-control"></textarea>
                    <span asp-validation-for="ChangeReason" class="text-danger"></span>
                </div>
            </div>
            <div class="row align-items-md-center border">
                <div class="form-group col-md-3">
                    <label>Priority</label>
                </div>
                <div class="form-group col-md-3">
                    <div class="checkbox">
                        <label>
                            <input asp-for="PriorityEmergency" /> @Html.DisplayNameFor(model => model.PriorityEmergency)
                        </label>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    <div class="checkbox">
                        <label>
                            <input asp-for="PriorityRoutine" /> @Html.DisplayNameFor(model => model.PriorityRoutine)
                        </label>
                    </div>
                </div>
                <div class="form-group col-md-3">
                    <div class="checkbox">
                        <label>
                            <input asp-for="PriorityNextVersion" /> @Html.DisplayNameFor(model => model.PriorityNextVersion)
                        </label>
                    </div>
                </div>
            </div>
            <div class="row align-items-md-center border">
                <div class="form-group col-md-6">
                    <label asp-for="Date" class="control-label"></label>
                    <input asp-for="Date" class="form-control" />
                    <span asp-validation-for="Date" class="text-danger"></span>
                </div>
                <div class="form-group col-md-6">
                    <label asp-for="ReferenceNumber" class="control-label"></label>
                    <input asp-for="ReferenceNumber" class="form-control" />
                    <span asp-validation-for="ReferenceNumber" class="text-danger"></span>
                </div>
            </div>
