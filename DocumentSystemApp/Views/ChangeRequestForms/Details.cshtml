                <div class="form-group col-md-2">
                    <div class="checkbox">
                        <label>
                            <input asp-for="ConfigurationFirmware" /> @Html.DisplayNameFor(model => model.ConfigurationFirmware)
                        </label>
                    </div>
                </div>
            </div>
            <div class="row align-items-center border">
                <div class="form-group col-md-2">
                    <label>Change Type 1</label>
                </div>
                <div class="form-group col-md-2">
                    <div class="checkbox">
                        <label>
                            <input asp-for="ChangeType1NewRequirement" /> @Html.DisplayNameFor(model => model.ChangeType1NewRequirement)
                        </label>
                    </div>
                </div>
                <div class="form-group col-md-2">
                    <div class="checkbox">
                        <label>
                            <input asp-for="ChangeType1RequirementChange" /> @Html.DisplayNameFor(model => model.ChangeType1RequirementChange)
                        </label>
                    </div>
                </div>
                <div class="form-group col-md-2">
                    <div class="checkbox">
                        <label>
                            <input asp-for="ChangeType1DesignChange" /> @Html.DisplayNameFor(model => model.ChangeType1DesignChange)
                        </label>
                    </div>
                </div>
                <div class="form-group col-md-2">
                    <div class="checkbox">
                        <label>
                            <input asp-for="ChangeType1Other" /> @Html.DisplayNameFor(model => model.ChangeType1Other)
                        </label>
                    </div>
                </div>
                <div class="form-group col-md-2">
                    <div class="checkbox">
                        <label>
                            <input asp-for="ChangeType1BugFixing" /> @Html.DisplayNameFor(model => model.ChangeType1BugFixing)
                        </label>
                    </div>
                </div>
            </div>


            <div class="row align-items-center border">
                <div class="form-group col-md-2">
                    <label>Change Type 2</label>
                </div>
                <div class="form-group col-md-2">
                    <div class="checkbox">
                        <label>
                            <input asp-for="ChangeType2Commercial" /> @Html.DisplayNameFor(model => model.ChangeType2Commercial)
                        </label>
                    </div>
                </div>
                <div class="form-group col-md-2">
                    <div class="checkbox">
                        <label>
                            <input asp-for="ChangeType2Investment" /> @Html.DisplayNameFor(model => model.ChangeType2Investment)
                        </label>
                    </div>
