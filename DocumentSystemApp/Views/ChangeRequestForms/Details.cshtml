                <div class="form-group col-md-2">
                    <label>Impact Assessment:</label>
                </div>
                <div class="form-group col-md-5">
                    <div class="checkbox">
                        <label>
                            <input asp-for="bImpactAssessmentToA" /> @Html.DisplayNameFor(model => model.bImpactAssessmentToA)
                        </label>
                    </div>
                    <textarea asp-for="ImpactAssessmentToA" class="form-control"></textarea>
                    <span asp-validation-for="ImpactAssessmentToA" class="text-danger"></span>
                </div>
                <div class="form-group col-md-5">
                    <div class="checkbox">
                        <label>
                            <input asp-for="bImpactAssessmentMA" /> @Html.DisplayNameFor(model => model.bImpactAssessmentMA)
                        </label>
                    </div>
                    <textarea asp-for="ImpactAssessmentMA" class="form-control"></textarea>
                    <span asp-validation-for="ImpactAssessmentMA" class="text-danger"></span>
                </div>
            </div>
            <div class="row align-items-center border">
                <div class="form-group col-md-4">
                    <div class="checkbox">
                        <label>
