                            <input asp-for="bImpactAssessmentTtE" /> @Html.DisplayNameFor(model => model.bImpactAssessmentTtE)
                        </label>
                    </div>
                    <textarea asp-for="ImpactAssessmentTtE" class="form-control"></textarea>
                    <span asp-validation-for="ImpactAssessmentTtE" class="text-danger"></span>
                </div>

                <div class="form-group col-md-4">
