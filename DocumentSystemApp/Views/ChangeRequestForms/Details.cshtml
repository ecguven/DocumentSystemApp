                <div class="form-group col-md-2">
                    <div class="checkbox">
                        <label>
                            <input asp-for="ConfigurationSystemDocumentation" /> @Html.DisplayNameFor(model => model.ConfigurationSystemDocumentation)
                        </label>
                    </div>
