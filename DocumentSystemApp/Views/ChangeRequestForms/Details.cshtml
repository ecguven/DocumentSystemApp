                                </tr>
                                <tr>
                                    <td>
                                        <input asp-for="FARole2" class="form-control" />
                                        <span asp-validation-for="FARole2" class="text-danger"></span>
                                    </td>
                                    <td>
                                        <input asp-for="FAPrintName2" class="form-control" />
                                        <span asp-validation-for="FAPrintName2" class="text-danger"></span>
                                    </td>
                                    <td>
                                        <input asp-for="FASignatureDate2" class="form-control" />
