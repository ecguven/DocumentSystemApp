                        </label>
                    </div>
                    <textarea asp-for="ImpactAssessmentOth" class="form-control"></textarea>
                    <span asp-validation-for="ImpactAssessmentOth" class="text-danger"></span>
                </div>

            </div>
            <div class="row align-items-center border">
                <div class="form-group col-md-2">
                    <label>Configuration:</label>
                </div>
                <div class="form-group col-md-2">
                    <div class="checkbox">
                        <label>
                            <input asp-for="ChangeAccept" /> @Html.DisplayNameFor(model => model.ChangeAccept)
                        </label>
                    </div>
                </div>
